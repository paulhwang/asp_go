

@{
    ViewBag.Title = "AccountSignIn";
}

<body>
    <h2>Account Sign In</h2>

    <section class="login_section">
        <p>
            Name:
            <input type="text"
                   class="login_name"
                   placeholder="Enter your name">
        </p>

        <p>
            Password:
            <input type="text"
                   class="login_password"
                   placeholder="Enter your password">
        </p>

        <button class="login_button">
            Login
        </button>
    </section>

    <script src="~/js/phwang_ajax_class.js"></script>
    <script src="~/js/phwang_link_class.js"></script>
    <script src="~/js/phwang_session_class.js"></script>
    <script src="~/js/phwang_class.js"></script>
    <script src="~/js/account_sign_in.js"></script>
</body>
